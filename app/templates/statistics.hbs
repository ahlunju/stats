<h3 class="statistics-header">
  Statistics
</h3>
<div class="stats">
  <table class="table table-striped table-bordered">
    <tbody>
      {{#each developer in model}}
        <tr><td>{{#link-to 'statistic' developer.login}}{{developer.name}}{{/link-to}}</td></tr>
      {{/each}}
    </tbody>
  </table>
</div>
<form class="developer-search-section form-group col-sm-8" {{action "getUserLogin" on="submit"}}>
  <label for="login">Search Developer:</label>
  {{input class="form-control" type="text" value=login placeholder="Search a Githuber"}}
  <button class="btn btn-primary" type="submit" {{action "getUserLogin"}}>Button</button>
</form>